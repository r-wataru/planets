<!DOCTYPE html>
<html>
  <head>
    <title>Planets</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
    <link rel="shortcut icon" href="/sapphire.ico" type="image/vnd.microsoft.icon" />
    <link rel="icon" href="/sapphire.ico" type="image/vnd.microsoft.icon" />
    <meta name="viewport" content="width=device-width" />
  </head>
  <body>
    <%= NavbarPresenter.new(self).render %>
    <div id="wrapper">
      <div id="sidebar-wrapper">
        <%= SidebarPresenter.new(self).render %>
      </div>
      <div id="page-content-wrapper">
        <div class="row">
          <div class="col-lg-12">
            <%= yield %>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
